{
    "compilerOptions": {
        "module": "es6",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "downlevelIteration": true,
        "target": "es5",
        "declaration": true,
        "sourceMap": true,
        "outDir": "dist/",
        "lib": ["es2019"]
    },
    "include": ["source/**/*"],
    "typedocOptions": {
        "name": "Iterable Observer",
        "excludeExternals": true,
        "excludePrivate": true,
        "readme": "./ReadMe.md",
        "plugin": ["typedoc-plugin-mdn-links"]
    }
}
